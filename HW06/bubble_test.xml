<?xml version="1.0"?>
<test-suite>
    <test-case>
        <name>sort(1)</name>
        <has-max-executions>1</has-max-executions>
        <max-executions>1000000</max-executions>
        <randomize>1</randomize>
        <input>
            <test-array><address>ARRAY</address><value>1</value></test-array>
            <test-value><address>LENGTH</address><value>1</value></test-value>
        </input>
        <output>
            <test-array points = "5"><address>ARRAY</address><value>1</value></test-array>
        </output>
    </test-case>
    <test-case>
        <name>sort(-7, -9)</name>
        <has-max-executions>1</has-max-executions>
        <max-executions>1000000</max-executions>
        <randomize>1</randomize>
        <input>
            <test-array><address>ARRAY</address><value>-7, -9</value></test-array>
            <test-value><address>LENGTH</address><value>2</value></test-value>
        </input>
        <output>
            <test-array points = "5"><address>ARRAY</address><value>-9, -7</value></test-array>
        </output>
    </test-case>
    <test-case>
        <name>sort(1, 2, 3, 4, 5, 6, 7)</name>
        <has-max-executions>1</has-max-executions>
        <max-executions>1000000</max-executions>
        <randomize>1</randomize>
        <input>
            <test-array><address>ARRAY</address><value>1, 2, 3, 4, 5, 6, 7</value></test-array>
            <test-value><address>LENGTH</address><value>7</value></test-value>
        </input>
        <output>
            <test-array points = "5"><address>ARRAY</address><value>1, 2, 3, 4, 5, 6, 7</value></test-array>
        </output>
    </test-case>
    <test-case>
        <name>sort(7, 6, 5, 4, 3, 2, 1)</name>
        <has-max-executions>1</has-max-executions>
        <max-executions>1000000</max-executions>
        <randomize>1</randomize>
        <input>
            <test-array><address>ARRAY</address><value>7, 6, 5, 4, 3, 2, 1</value></test-array>
            <test-value><address>LENGTH</address><value>7</value></test-value>
        </input>
        <output>
            <test-array points = "5"><address>ARRAY</address><value>1, 2, 3, 4, 5, 6, 7</value></test-array>
        </output>
    </test-case>
    <test-case>
        <name>sort(95, 28, -2, 2, -19, -13, -47, -78, 51, -72, 72, 51, 96, -94, -41, -94, -23, -10, 52, -8)</name>
        <has-max-executions>1</has-max-executions>
        <max-executions>1000000</max-executions>
        <randomize>1</randomize>
        <input>
            <test-array><address>ARRAY</address><value>95, 28, -2, 2, -19, -13, -47, -78, 51, -72, 72, 51, 96, -94, -41, -94, -23, -10, 52, -8</value></test-array>
            <test-value><address>LENGTH</address><value>20</value></test-value>
        </input>
        <output>
            <test-array points = "5"><address>ARRAY</address><value>-94, -94, -78, -72, -47, -41, -23, -19, -13, -10, -8, -2, 2, 28, 51, 51, 52, 72, 95, 96</value></test-array>
        </output>
    </test-case>
</test-suite>
